<section>
    {{#with service}}
    <div class="card text-bg-success text-center ">
        <div class="card-body">
            <h2>{{title}}</h2>
            <p>{{description}}</p>
            {{#each buttons}}
            <button class="btn btn-primary">
                <a href="{{link}}"></a>{{text}}</button>
            {{/each}}
        </div>
    </div>
    {{/with}}
</section>